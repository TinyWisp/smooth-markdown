{
  "name": "@smooth-vue-markdown-editor/vuetify-toolbar",
  "version": "1.0.0",
  "private": false,
  "author": "TinyWisp",
  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
