    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
