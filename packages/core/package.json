{
  "name": "@smooth-markdown/core",
  "version": "1.0.0",
  "private": false,
  "author": "TinyWisp",
  "keywords": [
    "vue",
    "markdown",
    "editor"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build:lib": "./build.types.sh && rollup --bundleConfigAsCjs --config build.lib.config.js"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.3.3",
    "css-scoped": "^1.0.0",
    "insert-css": "^2.0.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "overlayscrollbars": "^2.4.6",
    "overlayscrollbars-vue": "^0.5.6",
    "vue-affix": "^0.5.2",
    "vue-use-css": "^1.0.12",
    "vue3-perfect-scrollbar": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/types": "^7.21.3",
    "@rollup/plugin-alias": "^4.0.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "@codemirror/commands": "^6.2.4",
    "@codemirror/lang-markdown": "^6.1.1",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.12.0",
    "@types/markdown-it": "^13.0.7",
    "codemirror": "^6.0.1",
    "highlight.js": "^11.9.0",
    "markdown-it": "^13.0.1",
    "markdown-it-br": "^1.0.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "vue": "^3.2.47",
    "vue-use-active-scroll": "^1.1.1"
  },
  "exports": {
    ".": {
      "require": "./lib/core/index.umd.js",
      "import": "./lib/core/index.esm.js",
      "typings": "./lib/core/index.d.ts"
    },
    "./plugins": {
      "require": "./lib/plugins/index.umd.js",
      "import": "./lib/plugins/index.esm.js",
      "typings": "./lib/plugins/index.d.ts"
    },
    "./renderers": {
      "require": "./lib/renderers/index.umd.js",
      "import": "./lib/renderers/index.esm.js",
      "typings": "./lib/renderers/index.d.ts"
    },
    "./*": "./lib/*"
  }
}
